{
  "perplexity": {
    "type": "local",
    "enabled": true,
    "command": "npx",
    "args": [
      "-y",
      "@perplexity-ai/mcp-server"
    ],
    "env": {
      "PERPLEXITY_API_KEY": "${PERPLEXITY_API_KEY}",
      "PERPLEXITY_TIMEOUT_MS": "2000000"
    }
  },
  "flowspace": {
    "type": "local",
    "enabled": true,
    "command": "fs2",
    "args": [
      "mcp"
    ]
  }
}
