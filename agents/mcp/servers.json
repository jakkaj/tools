{
  "serena": {
    "type": "local",
    "enabled": true,
    "command": "uvx",
    "args": [
      "--from",
      "git+https://github.com/oraios/serena",
      "serena",
      "start-mcp-server",
      "--context",
      "ide-assistant",
      "--project",
      ".",
      "--enable-web-dashboard",
      "false"
    ],
    "env": {}
  },
  "context7": {
    "type": "local",
    "enabled": true,
    "command": "npx",
    "args": [
      "-y",
      "@upstash/context7-mcp"
    ]
  }
}
